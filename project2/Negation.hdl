/**
 * Negate a 16-bit number, be aware that first bit is the sign-bit
 */

CHIP Negation {
    IN in[16];
    OUT out[16];

    PARTS:
   // Put you code here:
   Not16(in=in, out=outx);
   Inc16(in=outx, out=out);
}